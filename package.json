{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "e2e-setup": "selenium-standalone install",
    "e2e-test": " NODE_ENV=test nightwatch",
    "e2e-server": "selenium-standalone start",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "heroku-postbuild": "npm run build",
    "start": "NODE_ENV=production npm run pre-start ",
    "pre-start": "babel-node Server/bin/www",
    "start:dev": " NODE_ENV=development babel-node Server/bin/www",
    "start:serv": "nodemon --exec babel-node Server/bin/www",
    "test": "NODE_ENV=test npm run migrate:test && NODE_ENV=test npm run seed:test && NODE_ENV=test ./node_modules/.bin/nyc --reporter=lcov --reporter=text mocha -t 100000 --require babel-core/register test/* --exit",
    "testClient": "NODE_ENV=test jest --u ./Client/client_test/",
    "test:travis": "NODE_ENV=test npm run test && npm run testClient",
    "seed:test": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate:undo:all",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.5",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.15.3",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.3",
    "jest-localstorage-mock": "^2.1.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "lodash.map": "^4.6.0",
    "morgan": "^1.8.2",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "passport": "^0.4.0",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.13.5",
    "sequelize-cli": "^3.0.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "validator": "^9.1.1",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-config-reactjs": "^1.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.2.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mockery": "^2.1.0",
    "nodemon": "^1.11.0",
    "redux-mock-store": "^1.3.0",
    "selenium-standalone": "^6.12.0",
    "uglify-js-brunch": "^2.10.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/Client/jest/test-env.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "<rootDir>/Client/jest/styleMock.js",
      "\\.(png|jpg|jpeg|gif|ttf|eot|svg)$": "<rootDir>/Client/jest/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChidinmaOrajiaku/HelloBooksProject.git"
  },
  "bugs": {
    "url": "https://github.com/ChidinmaOrajiaku/HelloBooksProject/issues"
  },
  "homepage": "https://github.com/ChidinmaOrajiaku/HelloBooksProject#readme",
  "description": ""
